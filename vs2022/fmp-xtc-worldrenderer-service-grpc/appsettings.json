
{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Endpoints": {
      "Http": {
        "Url": "http://*:18000"
      },
      "Https": {
        "Url": "https://*:19000",
        "Certificate": {
          "Path": "../../cers/xtc.crt",
          "KeyPath": "../../cers/xtc.key"
        }
      }
    }
  },

  "Database": {
    "Driver": "None"
  }

}
